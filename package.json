{
  "homepage": "https://lifo123.github.io/lifo-library/",
  "name": "@lifo123/library",
  "version": "1.7.6",
  "description": "Lifo library",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lifo123/lifo-library.git"
  },
  "scripts": {
    "flifo:utilities": "npx postcss src/styles/Main.css -o src/styles/output/Flifo.css --no-map",
    "noTailwind:styles": "tailwindcss -i src/styles/noTailwind.css -o src/styles/output/Flifo.noTailwind.pre.css --minify",
    "build": "pnpm run flifo:utilities && tsc",

    "dev": "astro dev --host",
    "preview": "astro build && astro preview",
    "deploy": "pnpm run build && npm publish --access public",
    "deploy:pages": "astro check && astro build && gh-pages -d Build --dotfiles"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Lifo",
  "license": "ISC",
  "devDependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22.9.0",
    "@types/react": "^19.1.6",
    "astro": "^5.14.1",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@nanostores/persistent": "^1.0.0",
    "@nanostores/react": "^0.8.0",
    "@tailwindcss/vite": "^4.1.11",
    "nanostores": "^0.11.3",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.1.11"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./Stores": "./dist/Stores/index.js",
    "./utils": "./dist/utils/index.js",
    "./UI": "./dist/UI/index.js",
    "./User": "./dist/Stores/User.Store.js",
    "./Styles": "./dist/styles/Flifo.css"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.4.0",
    "@google/genai": "^1.16.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/react-dom": "^19.1.6",
    "framer-motion": "^12.23.3"
  }
}