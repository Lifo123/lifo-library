  "peerDependencies": {
    "@nanostores/react": "^0.8.0",
    "nanostores": "^0.11.3",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
