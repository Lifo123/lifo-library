{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["src/components/*"],
      "@Layouts/*": ["src/layouts/*"],
      "@Pages/*": ["src/pages/*"],
      "@Assets/*": ["src/assets/*"],
      "@Lib/*": ["src/lib/*"],
      "@Context/*": ["src/context/*"],
      "@Blogs/*": ["src/blogs/*"]

    }
  }
}